{
  "name": "create nfts",
  "steps": [
    {
      "step": "externalSteps",
      "path": "01_deploy.scen.json"
    },
    {
      "step": "setState",
      "currentBlockInfo": {
        "blockTimestamp": "100,000"
      },
      "accounts": {
        "sc:my-contract": {
          "nonce": "0",
          "balance": "0",
          "esdt": {
            "str:NFT-123456": {
              "lastNonce": "0",
              "roles": [
                "ESDTRoleNFTCreate"
              ]
            }
          },
          "storage": {
            "str:nftTokenId": "str:NFT-123456"
          },
          "code": "file:../output/my-contract.wasm",
          "owner": "address:owner"
        }
      }
    },
    {
      "step": "scCall",
      "txId": "create-NFT-1",
      "tx": {
        "from": "address:owner",
        "to": "sc:my-contract",
        "function": "createNft",
        "arguments": [
          "str:NFT 1",
          "5000",
          "str:www.mycoolnft.com/nft1.jpg",
          "500"
        ],
        "gasLimit": "20,000,000",
        "gasPrice": "0"
      },
      "expect": {
        "out": [
          "1",
          "1"
        ],
        "status": "0",
        "message": "",
        "gas": "*",
        "refund": "*"
      }
    },
    {
      "step": "checkState",
      "accounts": {
        "sc:my-contract": {
          "nonce": "0",
          "balance": "0",
          "esdt": {
            "str:NFT-123456": {
              "lastNonce": "1",
              "roles": [
                "ESDTRoleNFTCreate"
              ]
            }
          },
          "storage": {
            "str:nftTokenId": "str:NFT-123456"
          },
          "code": "file:../output/my-contract.wasm"
        },
        "+": ""
      }
    }
  ]
}
